{
  "consumer": {
    "name": "Demo frontend"
  },
  "provider": {
    "name": "Demo backend"
  },
  "interactions": [
    {
      "description": "a request for all dogs",
      "providerState": "i have a list of dogs",
      "request": {
        "method": "GET",
        "path": "/dogs"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": [
          {
            "name": "Max",
            "age": 3
          },
          {
            "name": "Lassie",
            "age": 8
          }
        ]
      }
    },
    {
      "description": "a request for a single dog",
      "providerState": "i have a list of dogs",
      "request": {
        "method": "GET",
        "path": "/dogs/1",
        "matchingRules": {
          "$.path": {
            "match": "regex",
            "regex": "\\/dogs\\/\\d+"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "name": "Max",
          "age": 3
        }
      }
    },
    {
      "description": "a request to save a dog",
      "providerState": "i can save a dog",
      "request": {
        "method": "POST",
        "path": "/dogs",
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "name": "newDog",
          "age": 1
        }
      },
      "response": {
        "status": 201,
        "headers": {
          "Location": "/dogs/1"
        },
        "matchingRules": {
          "$.headers.Location": {
            "match": "regex",
            "regex": "\\/dogs\\/\\d+"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}